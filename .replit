modules = ["nodejs-20", "web", "postgresql-16"]

[nix]
channel = "stable-25_05"
packages = ["supabase-cli", "gh"]

[deployment]
deploymentTarget = "static"
run = ["npm", "run", "start"]
build = ["npm", "run", "build"]
publicDir = "dist"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev -- --config vite.replit.config.ts"

[agent]
integrations = ["javascript_database:1.0.0", "javascript_openai:1.0.0"]
expertMode = true

[[ports]]
localPort = 5000
externalPort = 5000

[[ports]]
localPort = 8080
externalPort = 80

[[ports]]
localPort = 34771
externalPort = 3000
